{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "clang++ - Build and debug active file",
      "type": "lldb",
      "request": "launch",
      "preLaunchTask": "C/C++: clang++ build active file",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "args": [
        "-arg1",
        "-arg2"
      ],
      "terminal": "integrated",
      "cwd": "${fileDirname}"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/u/cs/ugrad/nguyend/cs33",
      "program": "/u/cs/ugrad/nguyend/cs33/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}